# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant Vector Database Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_api_key_here  # Optional, for authenticated Qdrant instances
QDRANT_COLLECTION_NAME=my_rag_collection  # Optional, defaults to "my_rag_collection"

# Google Drive API Configuration (for RAG ingestion)
GOOGLE_SERVICE_ACCOUNT_FILE=credentials/your-service-account.json
GOOGLE_DRIVE_FOLDER_ID=your_google_drive_folder_id_here

# Optional: Campaigns folder name within the Drive folder
CAMPAIGNS_FOLDER_NAME=Campaigns
